{
  "name": "star-sdk",
  "version": "0.1.21",
  "private": false,
  "description": "Browser game SDK with built-in leaderboards, mobile-safe audio, and one-command deploy. No backend needed. Works on iOS Safari.",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "star-sdk": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "skills",
    "examples"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "homepage": "https://buildwithstar.com/docs/sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildwithstar/star-sdk.git",
    "directory": "packages/star-sdk"
  },
  "bugs": {
    "url": "https://github.com/buildwithstar/star-sdk/issues"
  },
  "keywords": [
    "star",
    "sdk",
    "games",
    "web",
    "audio",
    "canvas",
    "leaderboard",
    "deploy",
    "hosting"
  ],
  "dependencies": {
    "star-audio": "*",
    "star-canvas": "*",
    "star-leaderboard": "*",
    "star-multiplayer": "^0.0.2",
    "star-sdk-cli": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.5"
  }
}
